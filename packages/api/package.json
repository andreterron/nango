{
    "name": "@nangohq/api",
    "version": "0.2.1",
    "description": "Nango's unified API.",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "start": "tsc && node dist/lib/api/server.js",
        "worker-start": "tsc && node dist/lib/workers/worker.js",
        "debug": "export DEBUG=* && npm run start",
        "migrate": "knex migrate:latest --esm --knexfile db/knexfile.cjs",
        "migrate-list": "knex migrate:list --esm --knexfile db/knexfile.cjs",
        "migrate-make": "knex migrate:make --esm --knexfile db/knexfile.cjs"
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "https://github.com/NangoHQ/nango",
        "directory": "packages/api"
    },
    "license": "SEE LICENSE IN LICENSE FILE IN GIT REPOSITORY",
    "dependencies": {
        "amqplib": "^0.10.3",
        "axios": "^0.27.2",
        "cors": "^2.8.5",
        "ejs": "^3.1.5",
        "express": "^4.18.1",
        "express-winston": "^4.2.0",
        "knex": "^2.3.0",
        "pg": "^8.8.0",
        "posthog-node": "^2.1.0",
        "ts-node": "^10.9.1",
        "winston": "^3.8.2"
    },
    "engines": {
        "node": ">=16.7",
        "npm": ">=6.14.11"
    },
    "devDependencies": {
        "@types/amqplib": "^0.8.2",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/node": "^18.7.6",
        "typescript": "^4.7.4"
    }
}
